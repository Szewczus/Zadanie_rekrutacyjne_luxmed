<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.collections.FXCollections?>

<?import ewa.frontend.Dog?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ewa.frontend.SecondaryController">
    <children>
        <Label text="Secondary View" />
        <Label style="-fx-font: NORMAL 20 Tahoma;" text="Psy" GridPane.columnIndex="0" GridPane.rowIndex="0">
        </Label>
        <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1">
            <columns>
                <TableColumn prefWidth="122.0000057220459" text="Name">
                    <cellValueFactory><PropertyValueFactory property="name"/> </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="131.60003662109375" text="Age">
                    <cellValueFactory><PropertyValueFactory property="age"/> </cellValueFactory>
                </TableColumn>
            </columns>
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <Dog name="Jacob" age="2"/>
                    <Dog name="Isabella" age="1"/>
                    <Dog name="Ethan" age="3"/>
                    <Dog name="Emma" age="1"/>
                    <Dog name="Michael" age="3"/>
                </FXCollections>
            </items>
        </TableView>

        <Button fx:id="secondaryButton" text="Switch to Primary View" onAction="#switchToPrimary" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
